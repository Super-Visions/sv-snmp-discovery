			<methods>
				<method id="IsAttributeRelevantForSecurityLevel">
					<static>true</static>
					<access>public</access>
					<type>Custom</type>
					<code><![CDATA[	public static function IsAttributeRelevantForSecurityLevel(string $sAttCode, ?string $sSecLevel)
	{
		switch ($sAttCode) {
			case 'community':
				return empty($sSecLevel);
			case 'security_name':
			case 'context_name':
				return !empty($sSecLevel);
			case 'auth_passphrase':
			case 'auth_protocol':
				return $sSecLevel == 'authPriv' || $sSecLevel == 'authNoPriv';
			case 'priv_passphrase':
			case 'priv_protocol':
				return $sSecLevel == 'authPriv';
			default:
				return false;
		}
	}]]></code>
				</method>
				<method id="GetInitialStateAttributeFlags">
					<static>false</static>
					<access>public</access>
					<type>Overload-DBObject</type>
					<code><![CDATA[ public function GetInitialStateAttributeFlags($sAttCode, &$aReasons = array())
	{
		$iFlags = 0;
		switch ($sAttCode) {
			case 'priv_passphrase':
				if (empty($this->Get('priv_protocol'))) $iFlags |= OPT_ATT_READONLY;
			case 'auth_passphrase':
				if (empty($this->Get('auth_protocol'))) $iFlags |= OPT_ATT_READONLY;
			case 'auth_protocol':
			case 'priv_protocol':
			case 'security_name':
			case 'context_name':
			case 'community';
				if (!static::IsAttributeRelevantForSecurityLevel($sAttCode, $this->Get('security_level'))) $iFlags |= OPT_ATT_READONLY;
				break;
		}

		return $iFlags | parent::GetInitialStateAttributeFlags($sAttCode, $aReasons);
    }]]></code>
				</method>
				<method id="GetAttributeFlags">
					<static>false</static>
					<access>public</access>
					<type>Overload-DBObject</type>
					<code><![CDATA[ public function GetAttributeFlags($sAttCode, &$aReasons = array(), $sTargetState = '')
	{
