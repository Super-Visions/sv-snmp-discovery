		switch ($sAttCode) {
			case 'priv_passphrase':
				if (empty($this->Get('priv_protocol'))) $iFlags |= OPT_ATT_READONLY;
			case 'auth_passphrase':
				if (empty($this->Get('auth_protocol'))) $iFlags |= OPT_ATT_READONLY;
			case 'auth_protocol':
			case 'priv_protocol':
			case 'security_name':
			case 'context_name':
			case 'community';
				if (!static::IsAttributeRelevantForSecurityLevel($sAttCode, $this->Get('security_level'))) $iFlags |= OPT_ATT_READONLY;
				break;
		}

		return $iFlags | parent::GetAttributeFlags($sAttCode, $aReasons, $sTargetState);
    }]]></code>
				</method>
				<method id="ComputeValues">
					<static>false</static>
					<access>public</access>
					<type>Overload-DBObject</type>
					<code><![CDATA[ public function ComputeValues()
	{
		parent::ComputeValues();

		foreach ([
			'community',
			'security_name',
			'auth_passphrase',
			'auth_protocol',
			'priv_passphrase',
			'priv_protocol',
			'context_name',
		] as $sAttCode){
			if (!static::IsAttributeRelevantForSecurityLevel($sAttCode, $this->Get('security_level'))) $this->Set($sAttCode, null);
		}
    }]]></code>
